{
    "name": "isaacongoma/laravel-feed",
    "description": "Generate rss feeds",
    "keywords": [
        "spatie",
        "rss",
        "laravel",
        "laravel-feed"
    ],
    "homepage": "https://github.com/spatie/laravel-feed",
    "license": "MIT",
    "authors": [
        {
            "name": "Isaac Ongoma",
            "email": "isaacshisia@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "^3.8.x-dev",
        "spatie/phpunit-snapshot-assertions": "^2.0.0",
        "spatie/test-time": "^0.0.1"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Feed\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Feed\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\Feed\\FeedServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
